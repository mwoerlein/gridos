>
Dies ist das TEXTSEGMENT fuer den GridOS-MicroKernel "< @@ >"
<
_start:
    movl 0xb8f00, %eax
    addl 0x4a, %eax
    jmp drawLT
_halt:
    hlt
    jmp _halt
drawLT:
    movw 0x73c, (%eax)
    addl 4, %eax
    jmp drawAT
drawGT:
    addl 4, %eax
    movw 0x73e, (%eax)
    jmp _halt
drawAT:
    movw 0xf40, (%eax)
    addl 2, %eax
    movw 0xf40, (%eax)
    jmp drawGT
